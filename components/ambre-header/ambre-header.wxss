/* components/ambre-header/ambre-header.wxss */
@import '../../wxss/common.wxss';
@import '../../assets/iconfont/iconfont.wxss';
.header {
  width: 100%;
  z-index: 99;
  position: fixed;
}
.back {
  margin-left: 30rpx;
  border: 1rpx solid rgb(229, 229, 229);
  height: 63rpx;
  margin-top: 13rpx;
  border-radius: 50rpx;
  line-height: 43rpx;
  z-index: 99 !important;
}
.transparent {
  position: fixed;
  background: #00000000;
  z-index: 99;
}
.iconfont {
  display: inline-block;
  text-align: center;
  margin: 10rpx 0;
  width: 85.7rpx;
  height: 43rpx;
  vertical-align: middle;
}
.iconfont:first-child {
  font-size: 26rpx;
  border-right: 1rpx solid rgb(229, 229, 229);
}
.iconfont:last-child {
  font-size: 36rpx;
}
.title {
  position: absolute;
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
}
